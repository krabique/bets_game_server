<h2>Your accounts</h2>

<table>
  <tbody>
    <% @accounts.each do |account| %>
      <tr id="<%= account.amount_currency %>">
        <td class="amount"><%= account.amount %></td>
        <td class="currency"><%= account.amount_currency %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Create new account', new_account_path %>
